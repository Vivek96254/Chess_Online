{
  "name": "chess-online-server",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc && npm run copy-migrations",
    "copy-migrations": "mkdir -p dist/db/migrations && cp -r src/db/migrations/*.sql dist/db/migrations/",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.16.0",
    "@types/uuid": "^9.0.7",
    "bcrypt": "^6.0.0",
    "chess.js": "^1.0.0-beta.8",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.18.0",
    "socket.io": "^4.7.2",
    "typescript": "^5.3.2",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
